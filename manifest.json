{
  "manifest_version": 2,

  "name": "Comic Sans Extension",
  "description": "Changes all fonts on all webpages into Comic Sans",
  "version": "0.0.1",
	
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["inject.js"],
			"run_at": "document_end"
		}
  ],
	
  "permissions": [
    "*://*/*"
  ]
}
