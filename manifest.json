{
  "manifest_version": 2,

  "name": "Comic Sans Extension",
  "description": "Changes all text on all webpages into Comic Sans",
  "version": "1.0.0",
	
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["inject.js"],
			"run_at": "document_end"
		}
  ],
	
  "permissions": [
    "*://*/*"
  ]
}
